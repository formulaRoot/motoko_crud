type Time = int;
type Result = 
 variant {
   err: text;
   ok: text;
 };
type Profile = 
 record {
   age: nat;
   name: text;
   premium_user: bool;
   registration_date: Time;
 };
service : {
  create_profile: (Profile) -> ();
  read_profile: (principal) -> (opt Profile) query;
  update_profile: (Profile) -> (Result);
}
